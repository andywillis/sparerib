{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-eval": 0,
    "jsx-a11y/img-has-alt": 0,
    "default-case": 0,
    "func-names": 0,
    "consistent-return": 0,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "no-console": 0,
    "object-curly-newline": 0,
    "array-bracket-spacing": ["error", "never"],
    "comma-dangle": 0,
    "padded-blocks": 0,
    "arrow-body-style": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-tabs": 0,
    "no-restricted-syntax": [
      "error",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "generator-star-spacing": ["error", { "before": false, "after": true }]
  }
}
